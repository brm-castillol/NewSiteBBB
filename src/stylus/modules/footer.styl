.cont-logo-colom
	position absolute
	top -49px
	
footer
	background $Rojo  
	bottom 0
	font-family $RalewayL
	font-weight 100
	padding 10px 0
	position fixed                      
	text-align center
	text-transform uppercase
	width 100%

ul.cont-text-footer
	font-size 1rem
	margin 0
	padding 0
	
	li
		cursor pointer
		display inline-block
		list-style none
		margin-right 7px
		

		a
			color $Blanco
			font-weight 100
			text-decoration none
		
			&:hover
				color $Verde
				text-decoration none
		
	