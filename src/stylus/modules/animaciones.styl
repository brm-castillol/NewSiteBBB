.parte-azul
	$Azul
		

svg *
	fill-opacity 0
	stroke currentColor
	color $Azul

.relleno-fondo
	fill-opacity 0
	animation-fill-mode forwards
	animation-iteration 1
			  
	animation-name  FillIn
	animation-duration 4s, 1s, 1s
	animation-delay 0, 3.5s, 3.5s

@Keyframes FillIn
	from 
		fill-opacity 0 
	to 
		fill-opacity 1
